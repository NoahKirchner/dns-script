;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.auror.mil. root.auror.mil. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; 
;	name	server	- NS records
	IN	NS	ns.auror.mil.

; name	servers - A records
ns.auror.mil.	IN	A	10.14.53.200

; 10.14.0.0/8 - A records
host630.auror.mil.	IN	A	10.14.53.20
host730.auror.mil.	IN	A	10.14.53.10

